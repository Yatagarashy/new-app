\u0430\\u0437\\u044B\\u0432\\u0430\\u0435\\u0442\\u0441\\u044F \\u043A\\u0430\\u0436\\u0434\\u043E\\u043C\\u0443 \\u0438\\u0437 \\u043D\\u0438\\u0445. \\u041F\\u043E\\u0441\\u0442\\u043E\\u044F\\u043D\\u043D\\u043E \\u043F\\u0440\\u043E\\u0432\\u043E\\u0434\\u044F\\u0442\\u0441\\u044F \\u0442\\u0435\\u043A\\u0443\\u0449\\u0438\\u0435 \\u0440\\u0435\\u043C\\u043E\\u043D\\u0442\\u044B, \\u0431\\u043B\\u0430\\u0433\\u043E\\u0443\\u0441\\u0442\\u0440\\u043E\\u0439\\u0441\\u0442\\u0432\\u043E \\u043F\\u0440\\u0438\\u043B\\u0435\\u0433\\u0430\\u044E\\u0449\\u0438\\u0445 \\u0442\\u0435\\u0440\\u0440\\u0438\\u0442\\u043E\\u0440\\u0438\\u0439, \\u0437\\u0430 \\u043C\\u043E\\u0433\\u0438\\u043B\\u0430\\u043C\\u0438 \\u0438 \\u043F\\u0430\\u043C\\u044F\\u0442\\u043D\\u0438\\u043A\\u0430\\u043C\\u0438 \\u0443\\u0445\\u0430\\u0436\\u0438\\u0432\\u0430\\u044E\\u0442 \\u0448\\u043A\\u043E\\u043B\\u044C\\u043D\\u0438\\u043A\\u0438, \\u043C\\u043E\\u043B\\u043E\\u0434\\u0435\\u0436\\u044C \\u0438 \\u0442\\u0440\\u0443\\u0434\\u043E\\u0432\\u044B\\u0435 \\u043A\\u043E\\u043B\\u043B\\u0435\\u043A\\u0442\\u0438\\u0432\\u044B.\"}),/*#__PURE__*/_jsx(\"p\",{children:\"\\u0413\\u043B\\u0443\\u0441\\u0441\\u043A\\u0438\\u0439 \\u0440\\u0430\\u0439\\u043E\\u043D \\u2013 \\u044D\\u0442\\u043E \\u043A\\u0440\\u0430\\u0439 \\u0441\\u043C\\u0435\\u043B\\u044B\\u0445, \\u043E\\u0442\\u0432\\u0430\\u0436\\u043D\\u044B\\u0445 \\u0438 \\u0442\\u0440\\u0443\\u0434\\u043E\\u043B\\u044E\\u0431\\u0438\\u0432\\u044B\\u0445 \\u043B\\u044E\\u0434\\u0435\\u0439. \\u041D\\u0430 \\u043E\\u0442\\u043A\\u0440\\u044B\\u0442\\u043E\\u0439 \\u0432 \\u0446\\u0435\\u043D\\u0442\\u0440\\u0430\\u043B\\u044C\\u043D\\u043E\\u043C \\u0433\\u043E\\u0440\\u043E\\u0434\\u0441\\u043A\\u043E\\u043C \\u043F\\u0430\\u0440\\u043A\\u0435 \\u0410\\u043B\\u043B\\u0435\\u0435 \\u0421\\u043B\\u0430\\u0432\\u044B \\u0443\\u0432\\u0435\\u043A\\u043E\\u0432\\u0435\\u0447\\u0435\\u043D\\u0430 \\u043F\\u0430\\u043C\\u044F\\u0442\\u044C \\u043E \\u0413\\u0435\\u0440\\u043E\\u044F\\u0445 \\u0421\\u043E\\u0432\\u0435\\u0442\\u0441\\u043A\\u043E\\u0433\\u043E \\u0421\\u043E\\u044E\\u0437\\u0430 \\u0438 \\u0421\\u043E\\u0446\\u0438\\u0430\\u043B\\u0438\\u0441\\u0442\\u0438\\u0447\\u0435\\u0441\\u043A\\u043E\\u0433\\u043E \\u0422\\u0440\\u0443\\u0434\\u0430, \\u0443\\u0440\\u043E\\u0436\\u0435\\u043D\\u0446\\u0430\\u0445 \\u0440\\u0430\\u0439\\u043E\\u043D\\u0430.\"})]})]})});};export default MemoryPage;","map":{"version":3,"names":["React","jsx","_jsx","jsxs","_jsxs","MemoryPage","className","children","src","width","height","alt","align","style","marginRight","marginLeft","marginTop"],"sources":["C:/Диск D/Новая папка (4)/my-app/src/pages/MemoryPage.jsx"],"sourcesContent":["import React from 'react'\r\n\r\nconst MemoryPage = () => {\r\n  return (\r\n    <div className='MemoryPage'>\r\n        <div className='container'>\r\n            <h1 className='bold'>В память о погибших в годы Великой Отечественной войны</h1>\r\n            <div className='memory_block light'>\r\n                <p>\r\n                    <img src=\"https://glusk.gov.by/uploads/images/s000291_069485.jpg\" width=\"15%\" height=\"15%\" alt=\"\" align='left' style={{marginRight: '20px'}}/>\r\n                    27 июня, на пятый день войны, Глусский район был оккупирован немецко-фашистскими захватчиками и оказался в тылу группы армий «Центр». В июле 1941 года в районе Глуска вели тяжелые бои части 24-й Самара-Ульяновской Железной дивизии, 24 июля сводная кавалерийская группа 32-й кавалерийской дивизии освободила Глуск, разгромила гарнизон, но 27 июля отступила с тяжелыми боями. Наши земляки внесли свой весомый вклад в разгром врага. С первых дней оккупации началась освободительная борьба, на территории района успешно действовали 11 партизанских отрядов, объединивших свыше 1200 бойцов, подпольные райком партии, райком комсомола, издавалась подпольная газета «Сацыялістычная вёска».\r\n                </p>\r\n                <p>\r\n                    Наши земляки внесли свой весомый вклад в разгром врага. С первых дней оккупации началась освободительная борьба, на территории района успешно действовали 11 партизанских отрядов, объединивших свыше 1200 бойцов, подпольные райком партии, райком комсомола, издавалась подпольная газета «Сацыялістычная вёска».\r\n                </p>\r\n                <p>\r\n                    На фронт ушли шесть с половиной тысяч глусчан, из них не вернулись 4725, от рук оккупантов погибли 3126 мирных граждан, 1482 были угнаны на принудительные работы в Германию. Деревни Парщаха, Осы, Колесы, Белые, Мосты, Толстый Лес, сожженные вместе с жителями, повторили трагическую судьбу Хатыни. В декабре 1941 года на Мыслочанской горе были расстреляны более 2000 евреев и военнопленных.\r\n                </p>\r\n                <p>\r\n                    <img src=\"http://glusk.gov.by/uploads/images/s000291_647190.jpg\" width=\"20%\" height=\"20%\" alt=\"\" align='right' style={{marginLeft: '20px'}}/>\r\n                    Глусский район был освобожден 28 июня 1944 в ходе Бобруйской операции. Подвиг глусчан в годы Великой Отечественной войны увековечен в мемориале Воинская Слава.\r\n                </p>\r\n                <p>Символом немеркнущей славы стали 135 памятников и обелисков, в том числе на 95 воинских захоронениях (2 – периода Гражданской войны, 91 - погибших в годы Великой Отечественной войны, 2 – воинов-интернационалистов), наиболее крупные воинские захоронения расположены в деревнях Симоновичи, Макраны, Чикили, Славковичи, Заволочицы, Катка, на Хвастовичском воинском кладбище.</p>\r\n                <p>\r\n                    <img src=\"http://glusk.gov.by/uploads/images/s000291_610385.jpg\" width=\"20%\" height=\"20%\" align=\"left\" alt=\"\" style={{marginRight: '20px'}}/>\r\n                    В последние годы в результате поисковой работы, работы с обращениями граждан выявлено 13 ранее неизвестных воинских захоронений, список неизвестных защитников Отечества уменьшился на 135 имен.\r\n                    <img src=\"http://glusk.gov.by/uploads/images/s000291_925362.jpg\" width=\"20%\" height=\"20%\" alt=\"\" align=\"right\" style={{marginRight: '20px', marginTop: '20px'}}/>\r\n                </p>\r\n                <p>Одно из самых трагических событий в жизни нашего народа должно остаться в его памяти навсегда. Павшие герои и невинно погубленные дети, женщины, старики увековечены не только в людских сердцах, но и в памятных знаках, мемориалах, которых тысячи на многострадальной белорусской земле. Однако дань уважения и внимание общества оказывается каждому из них. Постоянно проводятся текущие ремонты, благоустройство прилегающих территорий, за могилами и памятниками ухаживают школьники, молодежь и трудовые коллективы.</p>\r\n                <p>\r\n                    \r\n                    Глусский район – это край смелых, отважных и трудолюбивых людей. На открытой в центральном городском парке Аллее Славы увековечена память о Героях Советского Союза и Социалистического Труда, уроженцах района.\r\n                </p>\r\n            </div>\r\n\r\n        </div>\r\n    </div>\r\n    \r\n  )\r\n}\r\n\r\nexport default MemoryPage"],"mappings":"AAAA,MAAO,CAAAA,KAAK,KAAM,OAAO,QAAAC,GAAA,IAAAC,IAAA,gCAAAC,IAAA,IAAAC,KAAA,yBAEzB,GAAM,CAAAC,UAAU,CAAG,QAAb,CAAAA,UAAUA,CAAA,CAAS,CACvB,mBACEH,IAAA,QAAKI,SAAS,CAAC,YAAY,CAAAC,QAAA,cACvBH,KAAA,QAAKE,SAAS,CAAC,WAAW,CAAAC,QAAA,eACtBL,IAAA,OAAII,SAAS,CAAC,MAAM,CAAAC,QAAA,CAAC,8RAAsD,CAAI,CAAC,cAChFH,KAAA,QAAKE,SAAS,CAAC,oBAAoB,CAAAC,QAAA,eAC/BH,KAAA,MAAAG,QAAA,eACIL,IAAA,QAAKM,GAAG,CAAC,wDAAwD,CAACC,KAAK,CAAC,KAAK,CAACC,MAAM,CAAC,KAAK,CAACC,GAAG,CAAC,EAAE,CAACC,KAAK,CAAC,MAAM,CAACC,KAAK,CAAE,CAACC,WAAW,CAAE,MAAM,CAAE,CAAC,CAAC,i3GAElJ,EAAG,CAAC,cACJZ,IAAA,MAAAK,QAAA,CAAG,sjDAEH,CAAG,CAAC,cACJL,IAAA,MAAAK,QAAA,CAAG,81DAEH,CAAG,CAAC,cACJH,KAAA,MAAAG,QAAA,eACIL,IAAA,QAAKM,GAAG,CAAC,uDAAuD,CAACC,KAAK,CAAC,KAAK,CAACC,MAAM,CAAC,KAAK,CAACC,GAAG,CAAC,EAAE,CAACC,KAAK,CAAC,OAAO,CAACC,KAAK,CAAE,CA